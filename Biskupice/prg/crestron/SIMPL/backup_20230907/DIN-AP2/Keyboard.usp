PUSH arm
{
	strArm_out=str;
	str="";
	gwiazdki="";
}

PUSH dissarm{
	strDiss_out=str;
	str="";
	gwiazdki="";
}
PUSH clear{
	str="";
	gwiazdki="";
}  
PUSH back {
 	str=left(str,len(str)-1); 
 	 
	gwiazdki=calcGwazdki();
} 

PUSH stars{
 	if (starsFB>0){
 	 	starsFB=0;
 	}else{
 	 	starsFB=1;
 	}              
	gwiazdki=calcGwazdki();
}

PUSH dig_i
{
	if (len(str)>4)
	{
		str=right(itoa(GetLastModifiedArrayIndex()),1);
	}
	else
	{
		str=str+right(itoa(GetLastModifiedArrayIndex()),1);
	}                                    
	gwiazdki=calcGwazdki();
}

